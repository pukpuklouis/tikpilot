{
  "name": "@tikpilot/playwright-service",
  "version": "1.0.0",
  "description": "Browser automation service using Playwright for TikPilot",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": ["playwright", "automation", "browser"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@playwright/test": "1.42.1",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.19.1",
    "express-async-handler": "1.2.0",
    "playwright": "1.42.1",
    "winston": "3.12.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/node": "20.11.30",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "eslint": "8.57.0",
    "prettier": "3.2.5",
    "tsx": "4.7.1",
    "typescript": "5.4.2",
    "vitest": "1.3.1"
  }
}
